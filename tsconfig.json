{
    "compilerOptions": {
        "target": "es2019",
        "module": "es2020",
        "strict": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "baseUrl": "./",
        "paths": {
          "*": ["src/*"]
        },
        "ignoreDeprecations": "5.0"
    },
    "extends": "@tsconfig/svelte/tsconfig.json",
    "include": ["src/**/*"],
    "exclude": ["node_modules/*", "__sapper__/*", "public/*"]
}
